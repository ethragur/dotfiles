export PATH="/usr/bin/:/usr/local/bin/:/usr/local/sbin:/usr/sbin/:$HOME/.rustup/toolchains/nightly-x86_64-unknown-linux-gnu/bin/:$HOME/go/bin/:$HOME/.local/bin/:/opt/bin/"
export JAVA_HOME="/usr/lib/jvm/openjdk/"
export LANG=en_US.UTF-8
export ARCHFLAGS="-arch x86_64"
export PLAYER=mpv
export EDITOR=nvim
export VISUAL=nvim
export GOPATH=$HOME/go/
export TERM=xterm

export GPG_TTY=$(tty)
export GTK_IM_MODULE=fcitx
export QT_IM_MODULE=fcitx
export XMODIFIERS="@im=fcitx"

source $HOME/.cargo/env
